{
    "name": "paskuale75/yii2-uppy",
    "description": "Wrapper for Uppy uploader jQuery widget",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Pasquale Pellicani",
            "email": "pellicanipasquale@gmail.com",
            "homepage": "http://www.paskuale.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "yiisoft/yii2": "~2.0.14",
        "bower-asset/commonjs-require": "*",
        "bower-asset/uppy": "@dev"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "paskuale75\\uppy\\": "src"
        }
    },
    "require-dev": {
    }
}
